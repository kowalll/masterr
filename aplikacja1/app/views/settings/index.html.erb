<p id="notice"><%= notice %></p>

<h1>Listing Settings</h1>
<table>
  <thead>
    <tr>
      <th>Order_title</th>
      <th>Show_order_then</th>
      <th>Show_active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setting.each do |setting| %>
      <tr>
        <td><%= setting.order_title %></td>
        <td><%= setting.show_order_then %></td>
        <td><%= setting.show_active %></td>
      </tr>
    <% end %>
  </tbody>
</table>

